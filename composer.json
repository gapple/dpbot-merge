{
    "name": "gapple/dpbot-merge",
    "type": "project",
    "require": {
        "php": "^7.3 || ^8.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "phpunit": "phpunit"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "sub/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": true
        }
    }
}
